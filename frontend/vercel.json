{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "status": 200
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://23-pwbcs-0973-wpfall-24.vercel.app/api/$1"
      }
    ],
    "env": {
      "VITE_BACKEND_URL": "https://23-pwbcs-0973-wpfall-24.vercel.app"
    }
  }
  